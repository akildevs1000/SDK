{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  //本机IP地址
  "LocalIP": "192.168.2.192",
  //是否启用UDP监听
  "EnableUDP": true,
  //UDP监听端口
  "UDPServerPort": 11001,
  //TCP监听端口
  "TCPServerPort": 11002,
  //设备UDP通讯端口
  "UDPPort": 8101,
  //设备TCP通讯端口
  "TCPPort": 8000,
  //命令重试次数
  "RestartCount": 1,
  //命令超时时间
  "Timeout": 5000,
  //连接设备的通讯密码
  "ConnectionPassword": "FFFFFFFF"
}
